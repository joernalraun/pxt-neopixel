{
    "name": "neopixel",
    "version": "0.6.15",
    "description": "AdaFruit NeoPixel driver",
    "license": "MIT",
    "dependencies": {
        "core": "*"
    },
    "files": [
        "README.md",
        "neopixel.ts",
        "sendBuffer.asm",
        "_locales/ja/neopixel-strings.json",
        "_locales/zh-cn/neopixel-strings.json",
        "_locales/zh-tw/neopixel-strings.json",
        "_locales/de/neopixel-strings.json",
        "_locales/de/neopixel-jsdoc-strings.json"
    ],
    "testFiles": [
        "neotest.ts"
    ],
    "public": true,
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "enabled": 1,
                    "pairing_mode": 1,
                    "private_addressing": 0,
                    "whitelist": 0,
                    "advertising_timeout": 0,
                    "tx_power": 6,
                    "dfu_service": 1,
                    "event_service": 0,
                    "device_info_service": 1,
                    "security_level": "SECURITY_MODE_ENCRYPTION_OPEN_LINK"
                }
            }
        }
    }
}
